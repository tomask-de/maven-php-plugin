<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.phpmaven.eclipse.feature"
      label="Phpmaven Feature"
      version="2.0.2.qualifier"
      provider-name="php-maven.org"
      plugin="org.phpmaven.eclipse.core">

   <description url="http://www.php-maven.org/tut-usingeclipse.html">
      PHP-Maven enables php to the maven world. This plugin bridges eclipse/pdt with maven by using php-maven.
   </description>

   <copyright url="http://www.php-maven.org">
      Copyright 2011 by php-maven.org
   </copyright>

   <license url="http://www.eclipse.org/org/documents/epl-v10.php">
      Eclipse public license V1.0
   </license>

   <url>
      <update label="PHPMaven update site" url="http://www.php-maven.org/eclipse/update"/>
      <discovery label="PHP-Maven home" url="http://www.php-maven.org"/>
      <discovery label="Official PHP-maven repository and project service provider" url="http://www.php-maven.org"/>
   </url>

   <requires>
      <import plugin="org.eclipse.php" version="3.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.m2e.core" version="1.0.100" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.phpmaven.eclipse.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.phpmaven.eclipse.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
