   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.phpmaven.eclipse.ui.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="org.phpmaven.eclipse.ui.menus.phpMaven"
               label="PHP-Maven"
               mnemonic="M">
            <menu
                  icon="icons/obj16/menu/lifecycle.png"
                  label="Lifecycle">
               <command
                     commandId="org.phpmaven.eclipse.ui.main.lifecycle.testCommand"
                     icon="icons/obj16/menu/lifecycle_test.png"
                     label="Test"
                     style="push">
               </command>
               <command
                     commandId="org.phpmaven.eclipse.ui.main.lifecycle.packageCommand"
                     icon="icons/obj16/menu/lifecycle_package.png"
                     label="Package"
                     style="push">
               </command>
               <command
                     commandId="org.phpmaven.eclipse.ui.main.lifecycle.installCommand"
                     icon="icons/obj16/menu/lifecycle_install.png"
                     label="Install"
                     style="push">
               </command>
               <command
                     commandId="org.phpmaven.eclipse.ui.main.lifecycle.deployCommand"
                     icon="icons/obj16/menu/lifecycle_upload.png"
                     label="Deploy"
                     style="push">
               </command>
               <command
                     commandId="org.phpmaven.eclipse.ui.main.lifecycle.releaseCommand"
                     icon="icons/obj16/menu/lifecycle_release.png"
