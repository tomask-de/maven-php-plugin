                  menubarPath="org.phpmaven.eclipse.ui.phpmavenMenu/org.phpmaven.eclipse.ui.siteMenu/group1">
            </action>
            <action
                  class="org.phpmaven.eclipse.ui.menus.SiteDeployHandler"
