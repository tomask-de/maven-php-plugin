                  label="Generate"
                  menubarPath="org.phpmaven.eclipse.ui.phpmavenMenu/org.phpmaven.eclipse.ui.siteMenu/group1">
            </action>
            <action
                  class="org.phpmaven.eclipse.ui.menus.WebViewHandler"
                  icon="icons/obj16/menu/web_view.png"
                  id="org.phpmaven.eclipse.ui.main.web.viewCommand"
                  label="View"
                  menubarPath="org.phpmaven.eclipse.ui.phpmavenMenu/org.phpmaven.eclipse.ui.webMenu/group1">
            </action>
            <action
                  class="org.phpmaven.eclipse.ui.menus.WebStopHandler"
                  icon="icons/obj16/menu/web_stop.png"
                  id="org.phpmaven.eclipse.ui.main.web.stopCommand"
                  label="Stop Webserver"
                  menubarPath="org.phpmaven.eclipse.ui.phpmavenMenu/org.phpmaven.eclipse.ui.webMenu/group1">
            </action>
            <action
                  class="org.phpmaven.eclipse.ui.menus.WebStartHandler"
                  icon="icons/obj16/menu/web_start.png"
                  id="org.phpmaven.eclipse.ui.main.web.startCommand"
                  label="Start Webserver"
                  menubarPath="org.phpmaven.eclipse.ui.phpmavenMenu/org.phpmaven.eclipse.ui.webMenu/group1">
            </action>
            <action
                  class="org.phpmaven.eclipse.ui.menus.InfoPhpMavenHandler"
                  id="org.phpmaven.eclipse.ui.main.info.phpMavenCommand"
                  label="PHP-Maven"
                  menubarPath="org.phpmaven.eclipse.ui.phpmavenMenu/org.phpmaven.eclipse.ui.infoMenu/group1">
            </action>
            <action
                  class="org.phpmaven.eclipse.ui.menus.InfoPhpHandler"
                  id="org.phpmaven.eclipse.ui.main.info.phpCommand"
                  label="PHP"
                  menubarPath="org.phpmaven.eclipse.ui.phpmavenMenu/org.phpmaven.eclipse.ui.infoMenu/group1">
            </action>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.phpmaven.eclipse.ui.category.phpmaven"
            name="PHP-Maven">
      </category>
      <view
            category="org.phpmaven.eclipse.ui.category.phpmaven"
            class="org.phpmaven.eclipse.ui.views.PhpUnitResultView"
            id="org.phpmaven.eclipse.ui.views.phpUnitResults"
            name="PHPUnit results"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.phpmaven.eclipse.ui.newCategory"
            name="PHP-Maven">
      </category>
      <wizard
            class="org.phpmaven.eclipse.ui.prj.NewProjectWizard"
            finalPerspective="org.eclipse.php.perspective"
            id="org.phpmaven.eclipse.ui.newProjectWizard"
            name="PHP-Maven project"
            project="true">
      </wizard>
      <primaryWizard
            id="org.phpmaven.eclipse.ui.newProjectWizard">
      </primaryWizard>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.phpmaven.eclipse.ui.commands.category"
            name="PHP-Maven">
      </category>
      <command
            categoryId="org.phpmaven.eclipse.ui.commands.category"
            defaultHandler="org.phpmaven.eclipse.ui.menus.LifecycleTestHandler"
            id="org.phpmaven.eclipse.ui.main.lifecycle.testCommand"
            name="Test">
      </command>
      <command
            categoryId="org.phpmaven.eclipse.ui.commands.category"
            defaultHandler="org.phpmaven.eclipse.ui.menus.LifecyclePackageHandler"
            id="org.phpmaven.eclipse.ui.main.lifecycle.packageCommand"
            name="Package">
      </command>
      <command
            categoryId="org.phpmaven.eclipse.ui.commands.category"
            defaultHandler="org.phpmaven.eclipse.ui.menus.LifecycleInstallHandler"
            id="org.phpmaven.eclipse.ui.main.lifecycle.installCommand"
            name="Install">
      </command>
      <command
            categoryId="org.phpmaven.eclipse.ui.commands.category"
            defaultHandler="org.phpmaven.eclipse.ui.menus.LifecycleDeployHandler"
            id="org.phpmaven.eclipse.ui.main.lifecycle.deployCommand"
            name="Deploy">
      </command>
      <command
            categoryId="org.phpmaven.eclipse.ui.commands.category"
            defaultHandler="org.phpmaven.eclipse.ui.menus.LifecycleReleaseHandler"
            id="org.phpmaven.eclipse.ui.main.lifecycle.releaseCommand"
            name="Release">
      </command>
      <command
