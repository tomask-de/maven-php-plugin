               <groupMarker
                     name="group1">
               </groupMarker>
            </menu>
            <menu
                  icon="icons/obj16/menu/web.png"
                  id="org.phpmaven.eclipse.ui.webMenu"
                  label="Web-Project"
                  path="org.phpmaven.eclipse.ui.phpmavenMenu/group1">
               <groupMarker
                     name="group1">
               </groupMarker>
            </menu>
            <menu
                  icon="icons/obj16/menu/site.png"
                  id="org.phpmaven.eclipse.ui.siteMenu"
                  label="Site"
                  path="org.phpmaven.eclipse.ui.phpmavenMenu/group1">
               <groupMarker
                     name="group1">
               </groupMarker>
            </menu>
            <menu
                  icon="icons/obj16/menu/lifecycle.png"
                  id="org.phpmaven.eclipse.ui.lifecycleMenu"
                  label="Lifecycle"
                  path="org.phpmaven.eclipse.ui.phpmavenMenu/group1">
               <groupMarker
                     name="group1">
               </groupMarker>
            </menu>
            <action
                  class="org.phpmaven.eclipse.ui.menus.LifecycleReleaseHandler"
                  icon="icons/obj16/menu/lifecycle_release.png"
                  id="org.phpmaven.eclipse.ui.main.lifecycle.releaseCommand"
                  label="Release"
                  menubarPath="org.phpmaven.eclipse.ui.phpmavenMenu/org.phpmaven.eclipse.ui.lifecycleMenu/group1">
            </action>
            <action
                  class="org.phpmaven.eclipse.ui.menus.LifecycleDeployHandler"
                  icon="icons/obj16/menu/lifecycle_upload.png"
                  id="org.phpmaven.eclipse.ui.main.lifecycle.deployCommand"
                  label="Deploy"
                  menubarPath="org.phpmaven.eclipse.ui.phpmavenMenu/org.phpmaven.eclipse.ui.lifecycleMenu/group1">
            </action>
            <action
                  class="org.phpmaven.eclipse.ui.menus.LifecycleInstallHandler"
                  icon="icons/obj16/menu/lifecycle_install.png"
                  id="org.phpmaven.eclipse.ui.main.lifecycle.installCommand"
                  label="Install"
                  menubarPath="org.phpmaven.eclipse.ui.phpmavenMenu/org.phpmaven.eclipse.ui.lifecycleMenu/group1">
            </action>
            <action
                  class="org.phpmaven.eclipse.ui.menus.LifecyclePackageHandler"
                  icon="icons/obj16/menu/lifecycle_package.png"
