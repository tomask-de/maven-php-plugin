      <command
            categoryId="org.phpmaven.eclipse.ui.commands.category"
            defaultHandler="org.phpmaven.eclipse.ui.menus.SiteViewHandler"
            id="org.phpmaven.eclipse.ui.main.site.viewCommand"
            name="View Report">
      </command>
      <command
            categoryId="org.phpmaven.eclipse.ui.commands.category"
            defaultHandler="org.phpmaven.eclipse.ui.menus.SiteDeployHandler"
            id="org.phpmaven.eclipse.ui.main.site.deployCommand"
            name="Deploy Report">
      </command>
      <command
            categoryId="org.phpmaven.eclipse.ui.commands.category"
            defaultHandler="org.phpmaven.eclipse.ui.menus.WebStartHandler"
            id="org.phpmaven.eclipse.ui.main.web.startCommand"
            name="Start Webserver">
      </command>
      <command
            categoryId="org.phpmaven.eclipse.ui.commands.category"
            defaultHandler="org.phpmaven.eclipse.ui.menus.WebStopHandler"
            id="org.phpmaven.eclipse.ui.main.web.stopCommand"
            name="Stop Webserver">
      </command>
      <command
            categoryId="org.phpmaven.eclipse.ui.commands.category"
            defaultHandler="org.phpmaven.eclipse.ui.menus.WebViewHandler"
            id="org.phpmaven.eclipse.ui.main.web.viewCommand"
            name="View Website">
      </command>
      <command
            categoryId="org.phpmaven.eclipse.ui.commands.category"
            defaultHandler="org.phpmaven.eclipse.ui.menus.InfoPhpHandler"
            id="org.phpmaven.eclipse.ui.main.info.phpCommand"
            name="Info PHP">
      </command>
      <command
            categoryId="org.phpmaven.eclipse.ui.commands.category"
            defaultHandler="org.phpmaven.eclipse.ui.menus.InfoPhpMavenHandler"
            id="org.phpmaven.eclipse.ui.main.info.phpMavenCommand"
            name="Info PHP-Maven">
      </command>
