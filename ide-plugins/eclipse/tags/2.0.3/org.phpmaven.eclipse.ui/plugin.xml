                     style="push">
               </command>
            </menu>
            <menu
                  icon="icons/obj16/menu/site.png"
                  label="Site">
               <command
                     commandId="org.phpmaven.eclipse.ui.main.site.generateCommand"
                     icon="icons/obj16/menu/site_generate.png"
                     label="Generate"
                     style="push">
               </command>
               <command
                     commandId="org.phpmaven.eclipse.ui.main.site.viewCommand"
                     icon="icons/obj16/menu/site_view.png"
                     label="View"
                     style="push">
               </command>
               <command
                     commandId="org.phpmaven.eclipse.ui.main.site.deployCommand"
                     icon="icons/obj16/menu/site_upload.png"
                     label="Deploy"
                     style="push">
               </command>
            </menu>
            <menu
                  icon="icons/obj16/menu/web.png"
                  label="Web-Project">
               <command
                     commandId="org.phpmaven.eclipse.ui.main.web.startCommand"
                     icon="icons/obj16/menu/web_start.png"
