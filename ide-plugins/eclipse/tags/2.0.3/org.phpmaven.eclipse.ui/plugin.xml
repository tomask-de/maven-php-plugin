                  label="Package"
                  menubarPath="org.phpmaven.eclipse.ui.phpmavenMenu/org.phpmaven.eclipse.ui.lifecycleMenu/group1">
            </action>
            <action
                  class="org.phpmaven.eclipse.ui.menus.LifecycleTestHandler"
                  icon="icons/obj16/menu/lifecycle_test.png"
                  id="org.phpmaven.eclipse.ui.main.lifecycle.testCommand"
                  label="Test"
                  menubarPath="org.phpmaven.eclipse.ui.phpmavenMenu/org.phpmaven.eclipse.ui.lifecycleMenu/group1">
            </action>
            <action
                  class="org.phpmaven.eclipse.ui.menus.SiteViewHandler"
