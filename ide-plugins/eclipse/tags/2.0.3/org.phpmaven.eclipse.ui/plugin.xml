                  id="org.phpmaven.eclipse.ui.main.site.deployCommand"
                  label="Deploy"
                  menubarPath="org.phpmaven.eclipse.ui.phpmavenMenu/org.phpmaven.eclipse.ui.siteMenu/group1">
            </action>
            <action
                  class="org.phpmaven.eclipse.ui.menus.SiteGenerateHandler"
                  icon="icons/obj16/menu/site_generate.png"
